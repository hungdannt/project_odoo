<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.config_parameter" id="mobile_schema_link" forcecreate="False">
            <field name="key">haverton_base.mobile_schema_link</field>
            <field name="value">hjmsb://haverton/web/reset_password?</field>
        </record>
        <record model="ir.config_parameter" id="appstore_url" forcecreate="False">
            <field name="key">haverton_base.appstore_url</field>
            <field name="value">https://www.apple.com/app-store/</field>
        </record>
        <record model="ir.config_parameter" id="google_play_url" forcecreate="False">
            <field name="key">haverton_base.google_play_url</field>
            <field name="value">https://play.google.com/</field>
        </record>
        <record model="ir.config_parameter" id="document_root_path" forcecreate="False">
            <field name="key">haverton_base.document_root_path</field>
            <field name="value">/data/</field>
        </record>
        <record model="ir.config_parameter" id="job_directory_number_desired_width" forcecreate="False">
            <field name="key">haverton_base.job_directory_number_desired_width</field>
            <field name="value">5</field>
        </record>
        <record model="ir.config_parameter" id="auth_oauth_authorization_header" forcecreate="False">
            <field name="key">auth_oauth.authorization_header</field>
            <field name="value">True</field>
        </record>
        <record model="ir.config_parameter" id="mail_domain">
            <field name="key">haverton_base.mail_domain</field>
            <field name="value">@havertonhomes.com.au</field>
        </record>
    </data>
</odoo>
